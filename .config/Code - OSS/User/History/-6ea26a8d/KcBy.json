{
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact",
    "postcss": "css"
  },
  "editor.fontFamily": "\"Iosevka\", \"JetBrains Mono\", 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "material-icon-theme",
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "editor.renderControlCharacters": true,
  "tabnine.experimentalAutoImports": true,
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.default.cStandard": "c17",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootPath}",
  "window.titleSeparator": " | ",
  "editor.guides.bracketPairs": true,
  "editor.inlayHints.fontFamily": "JetBrains Mono",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 3,
  "diffEditor.codeLens": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "terminal.external.linuxExec": "alacritty",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 3,
  "terminal.integrated.enableFileLinks": false,
  "editor.minimap.scale": 2,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 4,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.linkedEditing": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.languageServer": "Pylance",
  "telemetry.telemetryLevel": "off",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.formatting": "clangFormat",
  "haskell.manageHLS": "GHCup",
  "npm.keybindingsChangedWarningShown": true,
  "cmake.configureOnOpen": false,
  "git.suggestSmartCommit": false,
  "eslint.codeActionsOnSave.rules": null,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.LOG1": "default",
    "*.md": "default"
  },
  "explorer.confirmDragAndDrop": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "files.associations": {
    ".env": "env"
  },
  "workbench.sideBar.location": "right",
  "git.terminalAuthentication": false,
  "enable-proposed-api": ["ms-toolsai.jupyter"],
  "window.zoomLevel": 2,
  "files.autoSave": "off",
  "accessibility.signals.terminalBell": {
    "sound": "on"
  },
  "terminal.integrated.enableVisualBell": true
}
